    <script>
      $(document).ready(function() {
        var ctx = document.getElementById("yearChart").getContext("2d");
        var data = {
            labels: [ "March", "April", "May", "June", "July", "August", "September", "October", "November", "December", "January", "February"],
            datasets: [
              {
                  label: "My First dataset",
                  fillColor: "rgba(220,220,220,0.5)",
                  strokeColor: "rgba(220,220,220,0.8)",
                  highlightFill: "rgba(220,220,220,0.75)",
                  highlightStroke: "rgba(220,220,220,1)",
                  data: [65, 59, 80, 81, 56, 55, 40, 35, 56, 74, 42, 58]
              },
            ]
        };
        var myBarChart = new Chart(ctx).Bar(data);
      });
    </script>
    <script>
      $(document).ready(function() {
        var ctwk = document.getElementById("weekChart").getContext("2d");
        var data = {
            labels: [ "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"],
            datasets: [
              {
                  label: "My First dataset",
                  fillColor: "rgba(220,220,220,0.5)",
                  strokeColor: "rgba(220,220,220,0.8)",
                  highlightFill: "rgba(220,220,220,0.75)",
                  highlightStroke: "rgba(220,220,220,1)",
                  data: [ 80, 81, 55, 40, 74, 42, 58]
              },
            ]
        };
        var myBarChart = new Chart(ctwk).Bar(data);
      });
    </script>
    <script>
      $(document).ready(function() {
        dataUrl = 'api/data/all';
        $.get( dataUrl, function( out ) {
          var dataObj = [];
          l = out[0].length;
          for (i = 0; i < l; i++) {
            var point = {
              'x': out[1][i],
              'y': out[0][i]
            };
            console.log(point);
            dataObj.push(point);
              //console.log((propt + ': ' + data[propt].time + ':' + data[propt].sound));
          }
          var ctd = document.getElementById("dayChart").getContext("2d");
          var data5 = [{
            label: '',
            strokeColor: 'rgba(77, 180, 73, 0.3)',
            xValueType: "dateTime",
            data: dataObj
          }];
          var myBubbleChart = new Chart(ctd).Scatter(data5, {
              bezierCurve: true,
              showTooltips: true,
              scaleShowHorizontalLines: true,
              scaleShowLabels: true,
              scaleBeginAtZero: true,
              scaleType: "date",
              datasetStroke: false
          });

          });
        });
    </script>


